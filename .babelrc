{
  "plugins": [
    ["module-resolver", {
      "root": ["./"]
    }],
    "transform-es2015-modules-commonjs",
    "syntax-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }]
  ]
}